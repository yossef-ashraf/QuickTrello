{
    "name": "quickhelper/quicktrello",
    "description": "A Laravel package for quick integration with Trello API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "illuminate/support": ">=10.0",
        "illuminate/http": ">=10.0",
        "guzzlehttp/guzzle": ">=7.5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=10.0",
        "orchestra/testbench": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "QuickTrello\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "QuickTrello\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "QuickTrello\\QuickTrelloServiceProvider"
            ],
            "aliases": {
                "QuickTrello": "QuickTrello\\Facades\\QuickTrello"
            }
        }
    }
}